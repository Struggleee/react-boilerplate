  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,mjs,jsx,json,css,md}": [
      "prettier --single-quote --trailingComma all --write",
      "git add"
    ]
  }
