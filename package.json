    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "enquire.js": "^2.1.6",
    "history": "^4.7.2",
    "i18next": "^11.10.0",
    "i18next-browser-languagedetector": "^2.2.3",
    "jwt-decode": "^2.2.0",
